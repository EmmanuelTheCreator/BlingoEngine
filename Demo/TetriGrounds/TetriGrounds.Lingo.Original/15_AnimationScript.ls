property myEndMembernum,myStartMembernumproperty myValue,myStep,myDataSpriteNum,myDataName,myFunctionproperty myWaiter,myWaitbeforeExecute,myAnimate,mySlowDown,mySlowDownCounteron getPropertyDescriptionList  description = [:]  addProp description,#myStartMembernum, [#default:0, #format:#integer, #comment:"My Start membernum:"]  addProp description,#myEndMembernum, [#default:10, #format:#integer, #comment:"My End membernum:"]  addProp description,#myValue, [#default:-1, #format:#integer, #comment:"My Start Value:"]  addProp description,#mySlowDown, [#default:1, #format:#integer, #comment:"mySlowDown:"]  addProp description,#myDataSpriteNum, [#default:1, #format:#integer, #comment:"My Sprite that contains info\n(set value to -1):"]  addProp description,#myDataName, [#default:1, #format:#string, #comment:"Name Info:"]  addProp description,#myWaitbeforeExecute, [#default:0, #format:#integer, #comment:"WaitTime before execute:"]  addProp description,#myFunction, [#default:70, #format:#symbol, #comment:"function to execute:"]  return descriptionendon Beginsprite me  if myValue=-1 then    myValue = sendsprite(myDataSpriteNum ,#GetCounterStartData,myDataName)    if myValue=void then myValue =0    if myValue < myStartMembernum or myValue>myEndMembernum then myValue=myStartMembernum  end if  me.Updateme()  myWaiter = myWaitbeforeExecute  myAnimate = false  me.startAnim()end--on exitframe me--  if myWaiter<myWaitbeforeExecute then--    if myWaiter=myWaitbeforeExecute-1 then--      sendsprite(myDataSpriteNum ,myFunction,myDataName,myValue)--    end if--    myWaiter = myWaiter +1--  end ifon stepframe me  if myAnimate = true then    if mySlowDownCounter = mySlowDown then      mySlowDownCounter=0      if myValue<=myEndMembernum then        myValue = myValue+1        me.Updateme()      else        -- finished anim        _movie.actorList.deleteOne(me)        myAnimate = false      end if    else      mySlowDownCounter = mySlowDownCounter +1    end if  end ifendon startAnim me  repeat with i =myStartMembernum to myEndMembernum    member(i).preload()    end repeat    myAnimate = true    mySlowDownCounter=0    myValue = myStartMembernum    _movie.actorList.append(me)endon Updateme me  sprite(me.spritenum).membernum = (myValue)  myWaiter =0endon endsprite me  if _movie.actorList.getpos(me)>0 then _movie.actorList.deleteOne(me)end