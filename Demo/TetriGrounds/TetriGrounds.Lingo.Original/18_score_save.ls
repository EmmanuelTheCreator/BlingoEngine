property myURL, myNetID, myDone, myErr, phpErrproperty ancestoron new me  myURL = ""  myErr = 0  phpErr = 0  myDone = False  ancestor = new script("ClassSubscibe")    return meendon subscribe me  put "merde"endon setURL me, scriptURL  myURL = scriptURLendon postScore me, nam, scor  encrypt = me.encryptke(nam,scor)  encrypt2 = 123456 + random(123456)  myDone = False  myErr = 0  myNetID = postNetText(myURL, ["myName":nam, "myScore":scor, "myNewScoreButtton":"true","myStaticKey":encrypt,"myStaticKey2":encrypt2])  _movie.actorList.add(me)endon stepFrame me  if netDone(myNetID) then    myErr = netError(myNetID)    if myErr = "OK" then      phpErr = value(netTextResult(myNetID))      resultt = netTextResult(myNetID)      me.executeAllSubscibed(resultt)      put resultt    end if    myDone = True    _movie.actorList.deleteOne(me)  end ifendon test me  me.executeAllSubscibed("testing")endon getErr me  return myErrendon getPhpErr me  return phpErrendon isDone me  return myDoneendon destroy me  me.subscriberdestroy()  if _movie.actorList.getpos(me)<>0 then _movie.actorList.deleteOne(me)endon encryptke me,_name,_score  total = 0  repeat with i =1 to length(_name)    total = total +charToNum(_name.char[i])  end repeat  _score = string(_score)  repeat with i =1 to length(_score)    total = total +charToNum(_score.char[i])  end repeat  total = total *12  shiftreg= total  repeat with i=1 to 3    if bitAnd(shiftreg,1) then       flag1=1     else       flag1=0    end if    if bitAnd(shiftreg,2) then       flag2=1     else       flag2=0    end if    if bitAnd(shiftreg,3) then       flag3=1     else       flag3=0    end if    if bitAnd(shiftreg,5) then       flag4=1     else       flag4=0    end if    if bitAnd(shiftreg,7) then       flag5=1     else       flag5=0    end if    shiftreg2 =shiftreg *2    shiftreg2 = bitOr(bitXor(bitXor(bitXor(bitXor(flag1,flag2),flag3),flag4),flag5),shiftreg2)    shiftreg2 = bitand(2147483647,shiftreg2)    shiftreg =shiftreg2      end repeat    --put shiftreg,flag1,flag2,flag3,flag4,flag5  return shiftreg    end