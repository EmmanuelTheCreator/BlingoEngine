property myURL, myNetID, myDone, myErr, phpErr, myParenton new me,_parent  myParent = _parent  myURL = ""  myErr = 0  phpErr = 0  myDone = False  return meendon setURL me, scriptURL  myURL = scriptURLendon post me, myStartLevel,myStartLines  myDone = False  myErr = 0  myNetID = postNetText(myURL, ["myStartLevel":myStartLevel,"myStartLines":myStartLines])  _movie.actorList.add(me)endon stepFrame me  if netDone(myNetID) then    myErr = netError(myNetID)    if myErr = "OK" then      phpErr = value(netTextResult(myNetID))      resultt = netTextResult(myNetID)      put resultt    end if    myDone = True    _movie.actorList.deleteOne(me)  end ifendon getErr me  return myErrendon getPhpErr me  return phpErrendon isDone me  return myDoneendon destroy me  me.subscriberdestroy()  if _movie.actorList.getpos(me)<>0 then _movie.actorList.deleteOne(me)end 