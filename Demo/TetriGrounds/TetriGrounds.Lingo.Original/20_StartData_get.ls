property myURL, myNetID, myDone, myErr, myData ,myParentproperty myShowTypeon new me,_parent  myParent = _parent  myURL = ""  myErr = 0  myDone = False  myData = ""  myShowType = 1  return meendon setURL me, scriptURL  myURL = scriptURLendon download me  myErr = 0  myDone = False  scores = []  myNetID = getNetText(myURL)  _movie.actorList.add(me)endon stepFrame me  if netDone(myNetID) then    myErr = netError(myNetID)    if myErr = "OK" then      myData = netTextResult(myNetID)      myParent.DataLoaded(myData,me)    end if    myDone = True    _movie.actorList.deleteOne(me)  end ifendon GetData me  return myDataendon getErr me  return myErrendon isDone me  return myDoneendon destroy me  if _movie.actorList.getpos(me)<>0 then _movie.actorList.deleteOne(me)  myParent = 0end