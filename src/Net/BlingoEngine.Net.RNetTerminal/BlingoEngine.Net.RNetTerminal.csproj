<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>false</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup>
    <!-- NuGet Packaging Metadata -->
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>BlingoEngine.Net.RNetTerminal</PackageId>
    <Version>1.0.0</Version>
    <Authors>Emmanuel The Creator</Authors>
    <Company>The Community</Company>
    <Description>Console client for interacting with a BlingoEngine RNet host.</Description>
    <RepositoryUrl>https://github.com/EmmanuelTheCreator/BlingoEngine</RepositoryUrl>
    <PackageTags>BlingoEngine RNet Terminal</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Terminal.Gui" Version="2.0.0-develop.4559" />
  </ItemGroup>
    <ItemGroup>
    <ProjectReference Include="..\\BlingoEngine.Net.RNetClient.Common\\BlingoEngine.Net.RNetClient.Common.csproj" />
    <ProjectReference Include="..\\BlingoEngine.Net.RNetPipeClient\\BlingoEngine.Net.RNetPipeClient.csproj" />
    <ProjectReference Include="..\\BlingoEngine.Net.RNetProjectClient\\BlingoEngine.Net.RNetProjectClient.csproj" />
    <ProjectReference Include="..\\BlingoEngine.Net.RNetContracts\\BlingoEngine.Net.RNetContracts.csproj" />
    <ProjectReference Include="..\\..\\BlingoEngine.IO.Data\\BlingoEngine.IO.Data.csproj" />
  </ItemGroup>
</Project>

